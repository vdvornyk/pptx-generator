        </a:r>
        <a:r>
            <a:rPr lang="en-GB" sz="2200" smtClean="0"/>
            <a:t>(</a:t>
        </a:r>
        <a:r>
            <a:rPr lang="en-US" sz="2200" smtClean="0">
                <a:hlinkClick r:id="${story_link_id}"/>
            </a:rPr>
            <a:t>${story_code}</a:t>
        </a:r>
        <a:r>
            <a:rPr lang="en-US" sz="2200" smtClean="0"/>
            <a:t>)</a:t>
        </a:r>
        <a:endParaRPr lang="en-GB" sz="2200" smtClean="0"/>
