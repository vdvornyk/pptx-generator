        <a:spLocks noGrp="1" noChangeArrowheads="1"/>
    </p:cNvSpPr>
    <p:nvPr>
        <p:ph type="title"/>
        <p:custDataLst>
            <p:tag name="TEXT_TYPE" val="PAGE HEADING"/>
        </p:custDataLst>
    </p:nvPr>
</p:nvSpPr>
<p:spPr>
    <a:xfrm>
        <a:off x="463550" y="457200"/>
        <a:ext cx="8978900" cy="423863"/>
    </a:xfrm>
</p:spPr>
<p:txBody>
    <a:bodyPr/>
    <a:lstStyle/>
    <a:p>
        <a:r>
            <a:rPr lang="en-GB" sz="2200" smtClean="0"/>
            <a:t>Story </a:t>
        </a:r>
        <a:r>
            <a:rPr lang="en-GB" sz="2200" i="1" smtClean="0"/>
